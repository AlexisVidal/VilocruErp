                                                        <input type="file" name="FileUpload" id="FileUpload1" onChange="Handlechange();" style="margin-top:5px;" accept=".png , .jpg" />
                                                        <div class="horizontal-space"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </td>
                                    </tr>
